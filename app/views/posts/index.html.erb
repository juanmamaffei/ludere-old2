<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Grupo</th>
      <th>Usuario</th>
      <th>Respuesta</th>
      <th>Contenido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.grupo %></td>
        <td><%= post.usuario %></td>
        <td><%= post.respuesta %></td>
        <td><%= post.contenido %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_grupo_post_path %>
